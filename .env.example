# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Build Configuration
SKIP_ENV_VALIDATION=true

# eBay API Configuration - Common
EBAY_ENVIRONMENT=production
EBAY_MARKETPLACE_ID=EBAY_US
EBAY_REDIRECT_URI=https://your-domain.vercel.app/api/ebay/auth/callback
EBAY_DEV_ID=your_dev_id_here

# eBay API - MJT Account
EBAY_CLIENT_ID_MJT=your_client_id_here
EBAY_CLIENT_SECRET_MJT=your_client_secret_here
EBAY_REFRESH_TOKEN_MJT=your_refresh_token_here

# eBay API - Green Account
EBAY_CLIENT_ID_GREEN=your_client_id_here
EBAY_CLIENT_SECRET_GREEN=your_client_secret_here
EBAY_REFRESH_TOKEN_GREEN=your_refresh_token_here

# eBay Sandbox (Optional - for testing)
EBAY_SANDBOX_CLIENT_ID=your_sandbox_client_id_here
EBAY_SANDBOX_CLIENT_SECRET=your_sandbox_client_secret_here
EBAY_SANDBOX_REFRESH_TOKEN=your_sandbox_refresh_token_here
