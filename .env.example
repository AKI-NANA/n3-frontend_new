# eBay API 認証情報
# eBay Developer Programから取得
# https://developer.ebay.com/
EBAY_CLIENT_ID=your_ebay_client_id_here
EBAY_CLIENT_SECRET=your_ebay_client_secret_here
EBAY_REFRESH_TOKEN=your_ebay_refresh_token_here
EBAY_DEV_ID=your_ebay_dev_id_here
EBAY_APP_ID=your_ebay_app_id_here

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Cron認証（本番環境のみ）
# ランダムな秘密キーを生成: openssl rand -base64 32
CRON_SECRET=your_random_secret_key_here

# トークン暗号化（本番環境推奨）
# 32バイトのランダムキー: openssl rand -hex 32
ENCRYPTION_KEY=your_32_byte_encryption_key_here

# アプリケーションURL（GitHub Actions用）
APP_URL=https://your-app.vercel.app
